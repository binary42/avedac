<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <artifactId>aved</artifactId>
        <groupId>aved</groupId>
        <version>0.4.3-SNAPSHOT</version>
    </parent>

    <groupId>aved</groupId>
    <artifactId>aved-ui</artifactId>
    <name>AVEDac user interface</name>
    <version>0.4.3-SNAPSHOT</version>
    <url>http://maven.apache.org</url>
    <build>
        <!-- Grab the properties to filter by -->
        <filters>
            <filter>src/main/app-filters/${environment}.properties</filter> 
        </filters>   
        <!-- Make sure that filtering is turned on so when the properties files
			are copied, the proper substitutions happen -->
        <resources>
            <resource>
                <directory>src/main/app-resources</directory> 
                <filtering>true</filtering>
            </resource>           
            <resource>
                <directory>src/main/app-filters</directory>
                <filtering>true</filtering>
            </resource>       
            <resource>
                <directory>src/main/resources</directory>  
            </resource>                                 
        </resources> 
        <plugins>
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.7.1</version>
                <configuration>
                    <skip>true</skip>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-resources-plugin</artifactId>
                <version>2.2</version>
                <configuration>
                    <encoding>${project.build.sourceEncoding}</encoding>
                </configuration>
            </plugin> 
            <plugin> 
                <groupId>org.freehep</groupId>
                <artifactId>freehep-nar-plugin</artifactId>
                <version>2.0-alpha-11-SNAPSHOT</version>
                <extensions>true</extensions>  
                <configuration>
                <linker>
                    <name>g++</name> 
                </linker>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.3.2</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                    <encoding>${project.build.sourceEncoding}</encoding>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <profiles> 
         <!-- for incrementing the version in the About box dialog -->
        <profile>
            <id>version-increment</id>
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>increment version</id>
                                <!--<phase>generate-sources</phase>-->
                                <configuration>
                                    <includePluginDependencies>true</includePluginDependencies>
                                    <tasks>
                                        <taskdef name="jreleaseinfo" classname="ch.oscg.jreleaseinfo.anttask.JReleaseInfoAntTask" classpath="maven.plugin.dependency.classpath" />
                                        <jreleaseinfo className="VersionInfo" packageName="org.mbari.aved.ui" version="${project.version}" targetDir="${basedir}/src/main/java" buildNumFile="${basedir}/version.properties" buildNumProperty="buildnum" />
                                    </tasks>
                                </configuration>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                            </execution>
                        </executions>
                        <dependencies>
                            <dependency>
                                <groupId>ch.oscg.jreleaseinfo</groupId>
                                <artifactId>jreleaseinfo</artifactId>
                                <version>1.3.0</version>
                                <scope>compile</scope>
                            </dependency>
                        </dependencies>
                    </plugin>
                    <plugin>
                        <artifactId>maven-scm-plugin</artifactId>
                        <version>1.5</version>
                        <executions>
                            <execution>
                                <phase>package</phase>
                                <configuration>
                                    <includes>version.properties</includes>
                                    <message>incremented build version</message>
                                </configuration>
                                <goals>
                                    <goal>checkin</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <id>Linux32</id>
            <activation> 
                <os>
                    <name>Linux</name>
                    <arch>i386</arch>
                </os>
            </activation>     
            <properties>
                <environment>external</environment> 
            </properties>         
            <dependencies>
                <dependency>
                    <groupId>org.mbari.aved.classifier</groupId>
                    <artifactId>sharedlibjni</artifactId>
                    <version>0.4.3-SNAPSHOT</version>
                    <type>nar</type>
                    <classifier>i386-Linux-g++-jni</classifier>
                </dependency>
                <!-- This includes the matlab shared library -->
                <dependency>
                    <groupId>org.mbari.aved.classifier</groupId>
                    <artifactId>sharedlib</artifactId>
                    <version>0.4.3-SNAPSHOT</version>
                    <type>nar</type>
                    <classifier>i386-Linux-g++-shared</classifier>
                </dependency>
            </dependencies>
            <build>
                <plugins>
                    <!--This plugin will unpack and include the necessary libraries for
            deploying this on a separate machine-->
                    <plugin>
                        <groupId>org.mbari.aved.classifier</groupId>
                        <artifactId>NarMojo</artifactId>
                        <version>0.4.3-SNAPSHOT</version>
                        <configuration>
                            <name>Linux</name>
                            <arch>i386</arch>
                        </configuration>
                        <executions>
                            <execution>
                                <phase>compile</phase>
                                <goals>
                                    <goal>unpack</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                    <!-- This plugin signs all of the packaged jars-->
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <executions>
                            <!-- sign the jars -->
                            <execution>
                                <id>sign Jars</id>
                                <phase>verify</phase>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                                <configuration>
                                    <tasks>
                                        <!--
                      during the development, debug profile defined in ../pom.xml will cause
                      the jars to be signed by a self-certified dummy public key.
                      For release, you should define the real values in ~/.m2/settings.xml
                    -->
                                        <signjar jar="target/aved-ui-0.4.3-SNAPSHOT.jar"
                                                 alias="${mbari.sign.alias}" storepass="${mbari.sign.storepass}" keypass="${mbari.sign.keypass}" keystore="${mbari.sign.keystore}" />
                                        <fileset dir="target/" includes="**/.jar"/>

                                    </tasks>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                    <!-- This plugin creates an executable jar-->
                    <!-- The intermediate JAR file must be named differently than -->
                    <!-- the shade output JAR file to prevent duplicate warnings. -->
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-jar-plugin</artifactId>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>org.mbari.aved.ui.Application</mainClass>
                                    <addClasspath>true</addClasspath>
                                </manifest>
                                <!-- This is signed separately and included
                                in the assembly as a separate jar. Include in the
                                class path as its needed in the build-->
                                <manifestEntries>
                                    <Class-Path>javahelp-2.0.02.jar</Class-Path>
                                </manifestEntries>
                            </archive>
                        </configuration>
                    </plugin>

                    <!-- This plugin packages up the artifact in a jar that includes
                    its dependencies and to "shade" or the packages of  some of the
                    dependencies-->
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-shade-plugin</artifactId>
                        <version>1.2</version>
                        <executions>
                            <execution>
                                <phase>package</phase>
                                <goals>
                                    <goal>shade</goal>
                                </goals>
                            </execution>
                        </executions>
                        <configuration>
                            <artifactSet>
                                <excludes>
                                    <exclude>abeille:jh</exclude>
                                </excludes>
                            </artifactSet>
                        </configuration>
                    </plugin>

                    <plugin>
                        <artifactId>maven-assembly-plugin</artifactId>
                        <inherited>false</inherited>
                        <executions>
                            <execution>
                                <id>make-gui-assembly</id>
                                <phase>package</phase>
                                <goals>
                                    <goal>single</goal>
                                </goals>
                                <configuration> 
                                    <finalName>${project.build.finalName}</finalName>
                                    <descriptors>
                                        <descriptor>assembly/linux32-assembly.xml</descriptor>
                                    </descriptors>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>


                </plugins>
            </build>
        </profile>
