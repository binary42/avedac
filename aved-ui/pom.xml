                </dependency>
                <!-- This includes the matlab shared library -->
                <dependency>
                    <groupId>org.mbari.aved.classifier</groupId>
                    <artifactId>sharedlib</artifactId>
                    <version>0.4.3-SNAPSHOT</version>
                    <type>nar</type>
                    <classifier>i386-MacOSX-g++-shared</classifier>
                </dependency>
            </dependencies>
            <build>
                <plugins>
                    <!--This plugin will unpack and include the necessary libraries for
            deploying this on a separate machine-->
                    <plugin>
                        <groupId>org.mbari.aved.classifier</groupId>
                        <artifactId>NarMojo</artifactId>
                        <version>0.4.3-SNAPSHOT</version>
                        <configuration>
                            <name>MacOSX</name>
                            <arch>i386</arch>
                        </configuration>
                        <executions>
                            <execution>
                                <phase>compile</phase>
                                <goals>
                                    <goal>unpack</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>

                    <!-- This plugin will create an assembly of examples, 
                    and dynamic libraries needed for executing the  user interface -->
                    <plugin>
                        <artifactId>maven-assembly-plugin</artifactId>
                        <inherited>false</inherited>
                        <executions>
                            <execution>
                                <id>make-gui-assembly</id>
                                <phase>package</phase>
                                <goals>
                                    <goal>single</goal>
                                </goals>
                                <configuration>
                                    <finalName>${project.build.finalName}</finalName>
                                    <descriptors>
                                        <descriptor>assembly/mac32-assembly.xml</descriptor>
                                    </descriptors>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                    <!-- This plugin will create a Mac OSX package for 32-bit-->
                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>osxappbundle-maven-plugin</artifactId>
                        <version>1.0-alpha-2</version>
                        <configuration>
                            <bundleName>AVEDac</bundleName>
                            <mainClass>org.mbari.aved.ui.Application</mainClass>
                            <iconFile>${basedir}/src/main/app-resources/mac/aved-ui-logo.icns</iconFile>
                            <vmOptions>-Xms512M -Xmx1024M -d32 -Dcom.sun.media.jai.disableMediaLib=true</vmOptions>
                            <dictionaryFile>${basedir}/src/main/app-resources/mac/mac32/Info.plist.template</dictionaryFile>
                            <javaApplicationStub>${basedir}/src/main/app-resources/mac/mac32/JavaApplicationStub</javaApplicationStub>
                            <!-- These resources are now bundled into the assembly plugin above
                            The application requires jnilib and dylib files generated by th nar plugin.
                            Following installation with the package maker, the location
                            of jnilib and dylib files are specified in the Info.plist
                            DYLD_LIBRARY_PATH key.

                            These libraries should really be put into a Mac Framework,
                            but currently the  mojo.codehaus.org/osxappbundle-maven-plugin
                            doesn't support Frameworks so we will put into the assembly
                            ans set the DYLD_LIBRARY_PATH appropriately -->
                            <!--<additionalResources>
                                <fileSet>
                                    <directory>${basedir}/src/main/app-resources/mac</directory>
                                    <includes>
                                        <include>runOnce.sh</include>
                                    </includes>
                                </fileSet>
                                <fileSet>
                                    <directory>${basedir}/target/nar/lib/i386-MacOSX-g++/jni/</directory>
                                    <includes>
                                        <include>*.jnilib</include>
                                    </includes>
                                </fileSet>
                                <fileSet>
                                    <directory>${basedir}/target/nar/lib/i386-MacOSX-g++/shared</directory>
                                    <includes>
                                        <include>*.qtf</include>
                                        <include>*.dylib</include>
                                        <include>*.h</include>
                                        <include>readme.*</include>
                                    </includes>
                                </fileSet>
                            </additionalResources>-->
                        </configuration>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>bundle</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>    
                </plugins>
            </build>
        </profile>

